{
    "name": "Post.news Dark Mode & Accessibility",
    "author": "Joshua Spires",
    "description": "Dark mode and accessibility improvements for Post.news",
    "version": "1.3.3",
    "manifest_version": 2,
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "96": "icons/96.png",
      "128": "icons/128.png",
      "256": "icons/256.png" },
      "default_icon": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "96": "icons/96.png",
        "128": "icons/128.png",
        "256": "icons/256.png" },
      "default_title": "Post.news Dark Mode & Accessibility",
    "content_scripts": [{
        "js": ["scripts/script.js"],
        "matches": ["https://post.news/*"]
      }]
}
